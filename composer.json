{
    "name": "pianzhou/hyperf-serve",
    "description": "serve package for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "serve"
    ],
    "homepage": "https://github.com/pianzhou2021/hyperf-serve",
    "support": {
        "docs": "https://github.com/pianzhou2021/hyperf-serve",
        "issues": "https://github.com/pianzhou2021/hyperf-serve/issues",
        "pull-request": "https://github.com/pianzhou2021/hyperf-serve/pulls",
        "source": "https://github.com/pianzhou2021/hyperf-serve"
    },
    "require": {
        "php": ">=7.2",
        "hyperf/command": "~2.2.0"

    },
    "autoload": {
        "psr-4": {
            "PianZhou\\Hyperf\\Serve\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "PianZhou\\Hyperf\\Serve\\ConfigProvider"
        }
    }
}
